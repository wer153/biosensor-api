# Docker Compose Environment Variables for Testing
# Copy this file to .env to customize your local development environment

# AWS Configuration (for testing - replace with real values for production)
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=ap-northeast-2

# S3 Configuration
AWS_S3_BUCKET_NAME=your-biosensor-test-bucket
AWS_S3_PRESIGNED_URL_EXPIRY=3600

# JWT Configuration - CHANGE THESE IN PRODUCTION!
JWT_SECRET=super-secret-jwt-key-for-development-only-change-in-production
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_HOURS=24

# Application Configuration
APP_NAME=Biosensor API
APP_VERSION=0.1.0
DEBUG=true
ENVIRONMENT=development

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# CORS Configuration (allows all origins for development)
CORS_ALLOW_ORIGINS=*
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=*
CORS_ALLOW_HEADERS=*
